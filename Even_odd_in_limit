//even odd nums in limit
#include<stdio.h>
int main()
{
	int i,n;
	printf("Enter limit:");
	scanf("%d",&n);
	printf("Even numbers:\n");
	for(i=0;i<n;i++)
	{
		if(i%2==0)
		{
			printf("%d ",i);
		}
	}
	printf("\nOdd numbers:\n");
	for(i=0;i<n;i++)
	{
		if(i%2!=0)
		{
			printf("%d ",i);
		}
	}
	return 0;
}


